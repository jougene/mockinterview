  "nodemonConfig": {
    "restartable": false
  },
  "mocha": {
    "slow": 2000,
    "colors": true,
    "full-trace": true,
    "recursive": true,
    "spec": "test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Hexlet/hexlet-interview.git"
  }
