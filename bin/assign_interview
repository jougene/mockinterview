#!/usr/bin/env bash

INTERVIEW_ID=$1
INTERVIEWER_ID=$2

if [[ -z "$INTERVIEW_ID" ]];
  then echo "Provide INTERVIEW_ID";
  exit 1
fi

curl -qs -X POST -H 'content-type: application/json' \
  -d "{\"interviewer_id\": $INTERVIEWER_ID}" \
  http://localhost:3030/admin/interview/$INTERVIEW_ID/assign -vvv
